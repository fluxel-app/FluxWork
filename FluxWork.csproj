<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net472</TargetFramework>
		<RepositoryUrl>https://github.com/fluxel-app/FluxWork</RepositoryUrl>
		<PackageVersion>1.0.1</PackageVersion>
		<Version>1.0.1</Version>
		<AssemblyName>FluxWork</AssemblyName>
		<RootNamespace>FluxWork</RootNamespace>
		<PackageId>FluxWork</PackageId>
		<Authors>Fluxel UG</Authors>
		<Company>FluxWork</Company>
		<Product>FluxWork</Product>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Title>FluxWork</Title>
		<Copyright>2020 (c) Fluxel UG</Copyright>
		<PackageProjectUrl>https://github.com/fluxel-app/FluxWork</PackageProjectUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>hitwork fluxwork</PackageTags>
        <PackageLicenseExpression>CC-BY-4.0</PackageLicenseExpression>
        <Description>.NET WPF MVVM-C "Framework" (Eher: Spielwiese)</Description>
        <LangVersion>9</LangVersion>
    </PropertyGroup>

    <ItemGroup>
      <Reference Include="PresentationCore" />
      <Reference Include="PresentationFramework" />
      <Reference Include="System.Xaml" />
      <Reference Include="WindowsBase" />
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="LightInject" Version="6.4.0" />
      <PackageReference Include="LightInject.Annotation" Version="1.1.0" />
    </ItemGroup>

    <ItemGroup>
      <Page Include="Presentation\View\BaseWindow.xaml">
        <Generator>MSBuild:Compile</Generator>
      </Page>
    </ItemGroup>

</Project>
